
<div class="devNodeParameter">
    @if (DevGraphNodeParameter.IsOutput)
    {
        <div>@DevGraphNodeParameter.Name</div>
    }

    <div class="devParameterNode@(IsGenericType ? " genericParameterNode" : "")">
        <div class="devParameterNodeDraggable" draggable="true" @ondragstart="OnDragStart" @ondrag="OnDrag" @ondragend="OnDragEnd">
        </div>
    </div>

    @if (DevGraphNodeParameter.IsInput)
    {
        <div>@DevGraphNodeParameter.Name</div>
    }
</div>

